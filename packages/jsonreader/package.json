{
  "name": "jsonreader",
  "version": "1.0.1",
  "description": "A utility for reading and parsing JSON streams in real-time",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "json",
    "reader",
    "parser",
    "stream",
    "streaming",
    "real-time",
    "progressive",
    "esm"
  ],
  "author": "Justin Schroeder <justin@formkit.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/jsonreader.git"
  },
  "bugs": {
    "url": "https://github.com/username/jsonreader/issues"
  },
  "homepage": "https://github.com/username/jsonreader#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@formkit/utils": "^1.5.0",
    "json5": "^2.2.3"
  },
  "devDependencies": {
    "@types/json5": "^2.2.0",
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.2",
    "tsup": "^8.0.1",
    "vitest": "^3.0.8"
  }
} 